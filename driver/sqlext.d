private auto bookmarkValue() {
  version(Win64) {
    return SQL_C_TYPE_ID.SQL_C_UBIGINT;
  } else {
    return SQL_C_TYPE_ID.SQL_C_ULONG;
  }
}

private auto tcharValue() {
  version(UNICODE) {
    return SQL_C_TYPE_ID.SQL_C_WCHAR;
  } else {
    return SQL_C_TYPE_ID.SQL_C_CHAR;
  }
}

