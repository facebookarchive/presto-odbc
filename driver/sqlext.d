auto SQL_FUNC_EXISTS(T1, T2)(T1 pfExists, T2 uwAPI) {
  auto ptr = cast(UWORD*)(pfExists);
  auto offset = uwAPI >> 4;
  auto mask = 1 << (uwAPI & 0x000F);
  auto value = *(ptr + offset) & mask;
  return value ? SQL_TRUE : SQL_FALSE;
}

